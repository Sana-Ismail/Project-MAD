<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reminder_app">

    <application
        android:name=".channels"
        android:allowBackup="true"
        android:icon="@drawable/alarms"
        android:label="@string/app_name"
        android:roundIcon="@drawable/alarms"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FindReminder"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".TodayReminder"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EditReminder"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity2_addReminder"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".Alertreciever" />
    </application>

</manifest>